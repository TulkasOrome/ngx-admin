{
  "/api/*": {
    "target": "https://identitypulse-api-v3.azurewebsites.net",
    "secure": true,
    "changeOrigin": true,
    "logLevel": "debug",
    "headers": {
      "Origin": "https://identitypulse-api-v3.azurewebsites.net"
    }
  },
  "/azure-storage/*": {
    "target": "https://stidentitypulsebulk.blob.core.windows.net",
    "secure": true,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/azure-storage": ""
    },
    "headers": {
      "Origin": "https://stidentitypulsebulk.blob.core.windows.net"
    }
  }
}